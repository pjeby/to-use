{
  "name": "to-use",
  "version": "0.2.1",
  "description": "An ultralight, typesafe, read-consistent DI container that doesn't need decorators or reflection",
  "keywords": [
    "ioc",
    "di",
    "dependency injection",
    "container",
    "service container"
  ],
  "repository": "https://github.com/pjeby/to-use.git",
  "author": "PJ Eby",
  "license": "ISC",
  "files": [
    "cjs/to-use.js",
    "mjs/to-use.*s"
  ],
  "main": "cjs/to-use.js",
  "module": "mjs/to-use.js",
  "types": "mjs/to-use.d.ts",
  "scripts": {
    "build": "tsc --build",
    "watch": "mocha --watch",
    "test": "mocha && tsc --build"
  },
  "mocha": {
    "spec": "spec.ts",
    "watch-files": [
      "to-use.ts",
      "spec.ts",
      "README.md"
    ],
    "require": [
      "ts-node/register"
    ]
  },
  "devDependencies": {
    "@types/mocha": "^9.1.0",
    "@types/node": "^17.0.16",
    "mocha": "^9.2.0",
    "ts-node": "^10.5.0",
    "typescript": "4.4.3"
  }
}
